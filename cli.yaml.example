# Enclave CLI Configuration File
# This file can be placed in:
# - ./cli.yaml (current directory - highest precedence)
# - $HOME/.enclave/cli.yaml (user directory)
# - /etc/enclave/cli.yaml (system-wide - lowest precedence)

# API Server URL
# Can be overridden with: ENCLAVE_API_SERVER_URL env var or --api-url flag
api_server_url: https://api.enclave.io

# Authentication Configuration
# Can be overridden with:
# - ENCLAVE_AUTH_USERNAME and ENCLAVE_AUTH_PASSWORD env vars
# - --auth-username and --auth-password flags
auth:
  username: your-username
  password: your-password
